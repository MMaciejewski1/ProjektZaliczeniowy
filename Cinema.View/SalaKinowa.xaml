            <Button.Effect>
                <DropShadowEffect Color="#FF739CE2" Opacity="0.5" ShadowDepth="3"/>
            </Button.Effect>
        </Button>
        <Button Content="Dalej" HorizontalAlignment="Left" Margin="502,320,0,0" VerticalAlignment="Top" Width="114" Click="Button_Click_1" Height="30" IsDefault="True" Cursor="Hand">
            <Button.Effect>
                <DropShadowEffect Color="#FF739CE2" Opacity="0.7"/>
            </Button.Effect>
        </Button>
        <Button x:Name="buttonclear" Content="Usuń wszystkie rezerwacje" HorizontalAlignment="Left" Height="30" Margin="100,320,0,0" VerticalAlignment="Top" Width="166" Click="buttonclear_Click" Cursor="Cross">
            <Button.Effect>
                <BlurEffect Radius="2" RenderingBias="Quality"/>
            </Button.Effect>
        </Button>
    </Grid>
</Window>
