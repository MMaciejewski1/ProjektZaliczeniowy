        <Label Content="5" HorizontalAlignment="Left" Margin="84,268,0,0" VerticalAlignment="Top" Width="26" Height="30"/>
        <Label Content="Kolumny" HorizontalAlignment="Left" Margin="54,70,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="45"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label Content="1" HorizontalAlignment="Left" Margin="124,94,0,0" VerticalAlignment="Top" Width="16" Height="26"/>
        <Label Content="2" HorizontalAlignment="Left" Margin="199,94,0,0" VerticalAlignment="Top" Width="16" Height="26"/>
        <Label Content="3" HorizontalAlignment="Left" Margin="274,94,0,0" VerticalAlignment="Top" Width="16" Height="26"/>
        <Label Content="4" HorizontalAlignment="Left" Margin="349,94,0,0" VerticalAlignment="Top" Width="16" Height="26"/>
        <Label Content="5" HorizontalAlignment="Left" Margin="424,94,0,0" VerticalAlignment="Top" Width="16" Height="26"/>
