        <Label Content="Rzędy" HorizontalAlignment="Left" Margin="43,93,0,0" VerticalAlignment="Top" Width="44" Height="30" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="45"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label Content="1" HorizontalAlignment="Left" Margin="84,128,0,0" VerticalAlignment="Top" Height="30" Width="26"/>
        <Label Content="2" HorizontalAlignment="Left" Margin="84,162,0,0" VerticalAlignment="Top" Height="30" Width="26"/>
        <Label Content="3" HorizontalAlignment="Left" Margin="84,198,0,0" VerticalAlignment="Top" Height="30" Width="26"/>
